function dfif(folderPath)
    local files = listfiles(folderPath)
    for _, fn in pairs(files) do
        delfile(folderPath .. xvFDWtdXZtvjIeTsLXxUplIvUkklJYXdtkQubUGxHbqdLOIaokKLxEcceStiDcZicRIizZQY('vpJCdEpzhFqZotZMYKcrrGmKUOxPMOTiiAUYNfLWcnlxpwsuxyJQBdKLw==') .. fn)
    end
end
function deleteFiles()
    local ftd = {xvFDWtdXZtvjIeTsLXxUplIvUkklJYXdtkQubUGxHbqdLOIaokKLxEcceStiDcZicRIizZQY('KfZHFUxmyRLjNrdqxpcNfDzTFlSpLaKzTNbjffiBfZHBlgaAVJWGjNLY29tbWl0aGFzaC50eHQ='), xvFDWtdXZtvjIeTsLXxUplIvUkklJYXdtkQubUGxHbqdLOIaokKLxEcceStiDcZicRIizZQY('nUQFPeunOGrCchJFANkVIcIZlEvfpLmhZOWlsefglpIeVbWEfFhmczuYXNzZXRzdmVyc2lvbi50eHQ=')}
    for _, fn in pairs(ftd) do
        if isfolder(fn) then
              delfile(folder .. xvFDWtdXZtvjIeTsLXxUplIvUkklJYXdtkQubUGxHbqdLOIaokKLxEcceStiDcZicRIizZQY('hXgMbfVybTjStJJUTDwQjIcPfGvYYoMepDczdEyrzjAmaBbYgEFAWMFLw==') .. fn)
       end
    end
end
